# Environment variables for GymPT Nuxt app
# Copy to .env and fill in secrets for local development

# Database
DATABASE_URL=postgres://USER:PASSWORD@HOST:PORT/DBNAME

# AI Provider (Cloudflare Workers AI via OpenAI SDK)
AI_BASE_URL=https://api.cloudflare.com/client/v4/accounts/ACCOUNT_ID/ai/run
AI_API_KEY=your-cloudflare-token
AI_MODEL=llama-3.1-8b-instruct
